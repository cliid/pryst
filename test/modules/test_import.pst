// Test file for module imports and namespace handling
module test::client {
    // Import the web module
    import test::web;

    // Test using declaration
    using module test::web;

    export fn test_imports() -> bool {
        // Test direct qualified access
        const str server = test::web::SERVER_NAME;
        const int max = test::web::MAX_CONNECTIONS;

        // Test access through using declaration
        const str host = host();
        const int port = port();

        // Test string interpolation with imported values
        print("Server {server} running on {host}:{port} with max {max} connections");

        return true;
    }

    // Test block-level using
    export fn test_block_scope() -> bool {
        {
            using module test::web;
            const str local_host = host();
            print("Local scope host: {local_host}");
        }

        // This should not compile if uncommented (host() not in scope)
        // const str error_host = host();

        return true;
    }
}
