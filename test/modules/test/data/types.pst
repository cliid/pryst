module test::data::types {
    class Point {
        x: int;
        y: int;

        fn init(x: int, y: int) {
            this.x = x;
            this.y = y;
        }

        fn toString() -> string {
            return f"Point({this.x}, {this.y})";
        }
    }

    class Vector3D {
        x: float;
        y: float;
        z: float;

        fn init(x: float, y: float, z: float) {
            this.x = x;
            this.y = y;
            this.z = z;
        }

        fn toString() -> string {
            return f"Vector3D({this.x}, {this.y}, {this.z})";
        }
    }

    class Color {
        r: int;
        g: int;
        b: int;

        fn init(r: int, g: int, b: int) {
            this.r = r;
            this.g = g;
            this.b = b;
        }

        fn toString() -> string {
            return f"Color(r={this.r}, g={this.g}, b={this.b})";
        }
    }
}
