// Test block-level scope isolation
namespace pryst::math {
    module operations {
        fn add(int a, int b) -> int { return a + b; }
        fn subtract(int a, int b) -> int { return a - b; }
    }
}

namespace utils::math {
    module operations {
        fn multiply(int a, int b) -> int { return a * b; }
        fn divide(int a, int b) -> int { return a / b; }
    }
}

fn testScopeIsolation() -> void {
    {
        using module pryst::math::operations;
        int result1 = add(10, 5);
        print("Result1: {result1}");
    }

    {
        using module utils::math::operations;
        int result2 = multiply(5, 2);
        print("Result2: {result2}");
    }
}

fn main() -> void {
    testScopeIsolation();
}
