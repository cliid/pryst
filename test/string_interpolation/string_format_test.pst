// Test string interpolation with various types
fn main() -> int {
    let int_val = 123;
    let float_val = 45.67;
    let str_val = "test";

    print("Integer: {}", int_val);  // Should print: Integer: 123
    print("Float: {}", float_val);  // Should print: Float: 45.67
    print("String: {}", str_val);   // Should print: String: test

    // Test multiple values
    print("Values: {}, {}, {}", int_val, float_val, str_val);

    return 0;
}
