int main() {
    // Test integer printing
    int x = 42;
    print(x);

    // Test float printing
    float y = 3.14159;
    print(y);

    // Test string printing
    str message = "Hello, World!";
    print(message);

    // Test math function results printing
    print(sqrt(16.0));
    print(pow(2.0, 3.0));
    print(abs(-42));
    print(abs(-3.14));

    return 0;
}
