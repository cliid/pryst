// Simple test file to verify logger functionality
fn main() -> void {
    // This should generate various debug logs
    let x = 42;  // Should trigger debug log for variable declaration
    debug("Debug: x = " + x.toString());
    info("Info: x = " + x.toString());
    error("Error: x = " + x.toString());

    // Trigger some function entry/exit logs
    test_function();
}

fn test_function() -> void {
    // This function exists just to generate more debug logs
    debug("Entering test_function");
    let y = 10;
    info("Test function running with y = " + y.toString());
    error("Test error message from function");
    debug("Exiting test_function");
}
