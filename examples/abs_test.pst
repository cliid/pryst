int main() {
    // Test abs function with negative float
    float x = -42.0;
    print("Testing abs function:\n");
    print("abs(-42.0) = ");
    print(abs(x));
    print("\n");

    // Test abs function with positive float
    x = 15.5;
    print("abs(15.5) = ");
    print(abs(x));
    print("\n");

    // Test abs function with negative integer
    int y = -123;
    print("abs(-123) = ");
    print(abs(y));
    print("\n");

    return 0;
}
