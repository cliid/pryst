int main() {
    str text = "Hello, World!";
    print("Original string: ");
    print(text);
    print("\n");

    print("String length: ");
    print(length(text));
    print("\n");

    print("Substring (7, 5): ");
    print(substring(text, 7, 5));
    print("\n");

    str greeting = "Hello";
    str name = " Alice";
    print("Concatenated string: ");
    print(concat(greeting, name));
    print("\n");

    return 0;
}
