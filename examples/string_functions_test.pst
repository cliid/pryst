int main() {
    // Test string length
    str test = "Hello, World!";
    print(length(test));  // Should print 13

    // Test substring
    str sub = substring(test, 0, 5);  // Should get "Hello"
    print(sub);

    // Test string concatenation
    str greeting = "Hello, ";
    str name = "Pryst";
    str full = concat(greeting, name);  // Should get "Hello, Pryst"
    print(full);

    return 0;
}
