int main() {
    // Test sqrt function with both int and float
    int x = 16;
    float y = 25.0;
    print("sqrt(16) = ");
    print(sqrt(x));
    print("\n");
    print("sqrt(25.0) = ");
    print(sqrt(y));
    print("\n");

    // Test pow function with various combinations
    int base = 2;
    int exp = 3;
    print("pow(2, 3) = ");
    print(pow(base, exp));
    print("\n");

    float fbase = 2.5;
    float fexp = 2.0;
    print("pow(2.5, 2.0) = ");
    print(pow(fbase, fexp));
    print("\n");

    // Test abs function with both int and float
    int neg_int = -42;
    float neg_float = -3.14;
    print("abs(-42) = ");
    print(abs(neg_int));
    print("\n");
    print("abs(-3.14) = ");
    print(abs(neg_float));
    print("\n");

    return 0;
}
