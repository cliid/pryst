function main() {  
  println("<");
  println(4 < 20);  
  println(40 < 2);  
  println(4 < 4);  
  println(4 < 200000000000000000000000000);  
  println(40000000000000000000000000 < 20);  
  println(40000000000000000000000000 < 200000000000000000000000000);  
  println(400000000000000000000000000 < 20000000000000000000000000);  
  println(40000000000000000000000000 < 40000000000000000000000000);  

  println("<=");
  println(4 <= 20);  
  println(40 <= 2);  
  println(4 <= 4);  
  println(4 <= 200000000000000000000000000);  
  println(40000000000000000000000000 <= 20);  
  println(40000000000000000000000000 <= 200000000000000000000000000);  
  println(400000000000000000000000000 <= 20000000000000000000000000);  
  println(40000000000000000000000000 <= 40000000000000000000000000);  

  println(">");
  println(4 > 20);  
  println(40 > 2);  
  println(4 > 4);  
  println(4 > 200000000000000000000000000);  
  println(40000000000000000000000000 > 20);  
  println(40000000000000000000000000 > 200000000000000000000000000);  
  println(400000000000000000000000000 > 20000000000000000000000000);  
  println(40000000000000000000000000 > 40000000000000000000000000);  

  println(">=");
  println(4 >= 20);  
  println(40 >= 2);  
  println(4 >= 4);  
  println(4 >= 200000000000000000000000000);  
  println(40000000000000000000000000 >= 20);  
  println(40000000000000000000000000 >= 200000000000000000000000000);  
  println(400000000000000000000000000 >= 20000000000000000000000000);  
  println(40000000000000000000000000 >= 40000000000000000000000000);  

  println("==");
  println(4 == 20);  
  println(40 == 2);  
  println(4 == 4);  
  println(4 == 200000000000000000000000000);  
  println(40000000000000000000000000 == 20);  
  println(40000000000000000000000000 == 200000000000000000000000000);  
  println(400000000000000000000000000 == 20000000000000000000000000);  
  println(40000000000000000000000000 == 40000000000000000000000000);  

  println("!=");
  println(4 != 20);  
  println(40 != 2);  
  println(4 != 4);  
  println(4 != 200000000000000000000000000);  
  println(40000000000000000000000000 != 20);  
  println(40000000000000000000000000 != 200000000000000000000000000);  
  println(400000000000000000000000000 != 20000000000000000000000000);  
  println(40000000000000000000000000 != 40000000000000000000000000);  
}  
